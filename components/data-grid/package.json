{
  "name": "@amalto/data-grid",
  "displayName": "DataGrid",
  "description": "Grid used in platform6.",
  "version": "1.6.59",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amalto/platform6-ui-components"
  },
  "keywords": [
    "user-interface",
    "react",
    "typescript"
  ],
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "author": "Amalto Technologies",
  "scripts": {
    "setup": "npm install",
    "build": "rm -rf ./build/; NODE_ENV=production webpack -p --config webpack.config.js",
    "test": "ava ./test/test.js --verbose"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@amalto/action-button": "1.4.22",
    "@amalto/buttons-bar": "1.5.57",
    "@amalto/color-picker": "1.4.15",
    "@amalto/data-line": "1.4.18",
    "@amalto/helpers": "1.4.53",
    "@amalto/spinner": "1.4.16",
    "@amalto/toggle-panel": "1.4.21",
    "@amalto/wordings": "1.4.36",
    "classnames": "2.2.5",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-form": "6.6.1",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "1.1.0",
    "@types/react": "15.6.10",
    "@types/react-dom": "15.5.6",
    "@types/redux-form": "6.6.1",
    "@types/redux-thunk": "2.1.0",
    "ava": "0.24.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "15.6.2",
    "sinon": "4.2.2",
    "webpack": "3.10.0"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "@ava/stage-4",
        "react"
      ]
    }
  }
}
