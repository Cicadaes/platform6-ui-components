{
    "name": "@amalto/date-picker",
    "displayName": "DatePicker",
    "description": "DatePicker components.",
    "version": "1.0.18",
    "license": "MIT",
    "main": "./build/index.js",
    "typings": "./build/index.d.ts",
    "author": "Amalto Technologies",
    "publishConfig": {
