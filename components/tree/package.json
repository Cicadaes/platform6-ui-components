        "@types/react-dom": "0.14.23",
        "ts-loader": "3.2.0"
    },
    "dependencies": {
        "helpers": "1.1.4",
        "key-value-editor": "1.1.4",
        "react": "15.4.2",
        "react-dom": "15.4.2"
    }
