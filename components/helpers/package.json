{
    "name": "helpers",
    "displayName": "Helpers",
    "description": "Helpers functions used across b2portal core and components.",
    "version": "1.0.0",
    "main": "./build/index.js",
    "typings": "./build/index.d.ts",
    "author": "Amalto Technologies",
    "publishConfig": {
        "registry": "http://repo.amalto.com:8081/repository/npm-amalto/"
    },
    "scripts": {
        "setup": "npm install --registry http://repo.amalto.com:8081/repository/npm-all/",
        "start": "npm run storybook",
        "build": "npm run build:typescript && npm run build:storybook",
        "prepublish": "npm run build:typescript",
        "storybook": "../../node_modules/.bin/start-storybook -p 6006 -c .",
        "build:typescript": "../../node_modules/.bin/tsc",
        "build:storybook": "../../node_modules/.bin/build-storybook -c . -o static"
    },
    "dependencies": {
        "base-64": "^0.1.0"
    },
    "devDependencies": {
        "@types/base-64": "^0.1.2",
        "@types/bootstrap": "^3.3.36",
        "@types/jquery": "^3.2.15",
        "@types/modernizr": "^3.5.1"
    }
}