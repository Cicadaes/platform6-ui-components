        "@types/base-64": "0.1.2",
        "@types/bootstrap": "3.3.36",
        "@types/jquery": "2.0.41",
        "@types/modernizr": "3.5.1",
        "@types/react": "15.0.21",
        "@types/react-dom": "0.14.23"
    }
}
