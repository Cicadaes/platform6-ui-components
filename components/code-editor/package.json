{
    "name": "@amalto/code-editor",
    "displayName": "CodeEditor",
    "description": "Customizable code editor (non-form version).",
    "version": "1.1.35",
    "main": "./build/index.js",
    "typings": "./build/index.d.ts",
    "author": "Amalto Technologies",
    "publishConfig": {
        "registry": "http://repo.amalto.com:8081/repository/npm-amalto/"
    },
    "scripts": {
        "setup": "npm install --registry http://repo.amalto.com:8081/repository/npm-all/",
        "test": "ava ./test/test.js --verbose",
        "prepare": "tsc -d"
    },
    "devDependencies": {
        "@ava/babel-preset-stage-4": "1.1.0",
        "@types/ace": "0.0.36",
        "@types/bootstrap": "3.3.36",
        "@types/jquery": "3.3.0",
        "@types/react": "15.6.10",
        "@types/react-dom": "15.5.6",
        "ava": "0.24.0",
        "babel-preset-react": "6.24.1",
        "babel-register": "6.26.0",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-15": "1.0.5",
        "jquery": "3.3.1",
        "jsdom": "11.6.2",
        "react-addons-test-utils": "15.6.2",
        "react-test-renderer": "15.6.2",
        "sinon": "4.2.2",
        "typescript": "2.6.2"
    },
    "dependencies": {
        "@amalto/helpers": "1.1.26",
        "brace": "0.11.0",
        "react": "15.6.2",
        "react-dom": "15.6.2"
    },
    "ava": {
        "concurrency": 5,
        "failFast": true,
        "failWithoutAssertions": false,
        "powerAssert": true,
        "require": [
            "babel-register",
            "./test/utils.js"
        ],
        "babel": {
            "presets": [
                "@ava/stage-4",
                "react"
            ]
        }
    }
}
